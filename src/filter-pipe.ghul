namespace Ghul.Pipes is
    use Collections;

    class FilterPipe[T]: FilterPipeBase[T] is
        _predicate: T -> bool;

        init(enumerator: Iterator[T], predicate: T -> bool) is
            super.init(enumerator);

            _predicate = predicate;
        si

        should_include() -> bool => _predicate(current);
    si
si
